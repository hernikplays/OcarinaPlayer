<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxWidth="-Infinity" minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cz.hernikplays.Ocarina.Controller">
    <top>
        <MenuBar BorderPane.alignment="CENTER">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#openFile" text="Open" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </top>
    <bottom>
        <AnchorPane prefHeight="48.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <children>
                <Button layoutX="66.0" mnemonicParsing="false" onAction="#play" prefHeight="60.0" prefWidth="66.0" text="Play" />
            <Button layoutX="132.0" mnemonicParsing="false" onAction="#skip" prefHeight="60.0" prefWidth="66.0" text="Next" />
            <Button mnemonicParsing="false" onAction="#prev" prefHeight="60.0" prefWidth="66.0" text="Previous" />
            <Slider fx:id="volumeSlider" blockIncrement="1.0" layoutX="215.0" layoutY="23.0" majorTickUnit="1.0" max="1.0" minorTickCount="0" onMouseReleased="#changeVolume" value="1.0" />
            <Label layoutX="271.0" layoutY="6.0" text="Volume" />
            </children>
        </AnchorPane>
    </bottom>
</BorderPane>
